#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    scanf("%d",&n);
    double A[n+1];


    double sum=0;
    for( int i=1 ; i<=n ; i++ )
    {
        scanf("%lf",&A[i]);
        sum+=A[i];
    }

    for( int i=1 ; i<=n-1 ; i++ )
    {
        for( int j=i+1 ; j<=n ; j++ )
        {
            for( int k=i ;k<=j ; k++ ) sum+=A[k];
        }
    }

    printf("%.2lf",sum);

    return 0;

}
